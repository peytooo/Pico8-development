pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
 function _init()

 player={
	 x=63,
	 y=63,
	 f=false
	 }

end


function _update()
-- player movement --
	if btn(➡️) then
		player.x+=1
		player.f=true
	end
	if btn(⬅️) then
		player.x-=1
		player.f=false
	end
	--y movement--	
	if btn(⬇️) then
		player.y+=1
	end
	if btn(⬆️) then
		player.y-=1
	end
		
end


function _draw()
	cls(3)
	map()
	spr(1,player.x,player.y,1,1,player.f)
end
